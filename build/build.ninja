 description = 'Generating$ gsettings-compile-data$ with$ a$ custom$ command.'

# Test rules

build test: CUSTOM_COMMAND all PHONY
 COMMAND = '/usr/bin/python3' '/home/localhero/.local/bin/mesontest' '--no-rebuild' '--print-errorlogs'
