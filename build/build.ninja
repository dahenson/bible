 description = $DESC
 deps = gcc
 depfile = $DEPFILE
 restat = 1

rule REGENERATE_BUILD
 command = '/usr/bin/python3' '/home/localhero/.local/bin/meson' --internal regenerate '/home/localhero/Projects/Python/bible' '/home/localhero/Projects/Python/bible/build' --backend ninja
 description = Regenerating build files
